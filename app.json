{
  "expo": {
    "name": "therapymate",
    "slug": "therapymate",
    "version": "2.0.0",
    "orientation": "default",
    "userInterfaceStyle": "automatic",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.therapymatellc.therapymate",
      "versionCode": 2
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
